{
  "version": "0.1",
  "pipeline": "qml-classifier",
  "qnn": { "type": "estimator" },
  "dataset": {
    "type": "synthetic-line",
    "num_samples": 20,
    "num_features": 2,
    "rule": "x_plus_y_ge_0",
    "test_size": 0.2,
    "seed": 42
  },
  "encoder": { "type": "zzfeaturemap" },
  "circuit": { "type": "realamplitudes", "num_qubits": 2, "reps": 1 },
  "optimizer": { "type": "cobyla", "maxiter": 60 },
  "outputs": { "return_predictions": true, "return_generated_code": true }
}
